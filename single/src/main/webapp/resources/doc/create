ALTER TABLE single_search_log
	ADD
		CONSTRAINT PK_single_search_log -- 검색로그 기본키
		PRIMARY KEY (
			search_log_num -- 검색로그번호
		);

-- 찜한모임
CREATE TABLE single_stamp (
	user_id   varchar2(64) NOT NULL, -- 유저아이디
	group_num number       NOT NULL  -- 모임번호
);

-- 찜한모임
