	search_log_num number         NOT NULL, -- 검색로그번호
	user_id        varchar2(64)   NULL,     -- 유저아이디
	search_content varchar2(1024) NULL,     -- 검색내용
	search_date    DATE           NULL,     -- 검색일
	search_site    varchar2(256)  NULL      -- 검색사이트
);

