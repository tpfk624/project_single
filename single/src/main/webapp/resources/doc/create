	calendar_subject varchar2(256) NULL,     -- 모임일정제목
	calendar_content CLOB          NULL,     -- 모임일정내용
	calendar_date    DATE          NULL,     -- 일정날짜
	calendar_x_loc   float         NULL,     -- 일정장소X좌표
	calendar_y_loc   float         NULL      -- 일정장소Y좌표
