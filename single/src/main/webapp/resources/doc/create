	ADD
		CONSTRAINT PK_single_stamp -- 찜한모임 기본키
		PRIMARY KEY (
			user_id,   -- 유저아이디
			group_num  -- 모임번호
		);

-- 소셜로그인
CREATE TABLE sns_login (
	sns_id           VARCHAR2(45)  NOT NULL, -- 소셜아이디
	sns_type         VARCHAR(32)   NOT NULL, -- 소셜타입
	user_id          varchar2(64)  NOT NULL, -- 유저아이디
	sns_token        VARCHAR2(128) NOT NULL, -- 갱신토큰
	sns_connect_date VARCHAR2(45)  NULL,     -- 소셜접속일
	sns_email        VARCHAR2(45)  NULL,     -- 소셜이메일
	sns_nickname     VARCHAR2(45)  NULL,     -- 소셜닉네임
	sns_profile      VARCHAR2(45)  NULL,     -- 소셜이미지
	sns_age          VARCHAR2(45)  NULL,     -- 소셜나이
	sns_birthday     VARCHAR2(45)  NULL,     -- 소셜생일
	sns_name         VARCHAR2(45)  NULL,     -- 소셜이름
	sns_gender       VARCHAR2(45)  NULL      -- 소셜성별
);

-- 소셜로그인
ALTER TABLE sns_login
	ADD
		CONSTRAINT PK_sns_login -- 소셜로그인 기본키
		PRIMARY KEY (
			sns_id,   -- 소셜아이디
			sns_type  -- 소셜타입
		);

